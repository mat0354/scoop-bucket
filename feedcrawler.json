{
    "version": "11.1.7",
    "description": "FeedCrawler (ehemals RSScrawler) automatisiert bequem das Hinzufügen von Links für den JDownloader.",
    "homepage": "https://github.com/rix1337/FeedCrawler",
    "license": "MIT License",
    "architecture": {
        "64bit": {
            "url": "https://github.com/rix1337/FeedCrawler/releases/download/v.11.1.7/feedcrawler-11.1.7-standalone-win64.exe#/feedcrawler.exe"
        }
    },
    "bin": "feedcrawler.exe",
    "persist": [
        "config"
    ],
    "installer": {
        "script": [
            "\".\\config\" | Out-File -FilePath \"$original_dir\\feedcrawler.conf\" -Encoding ascii -NoNewline"
        ]
    },
    "checkver": {
        "github": "https://github.com/rix1337/FeedCrawler",
        "regex": "\/releases\/tag\/(?:v|V)\\.?([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/rix1337/FeedCrawler/releases/download/v.$version/feedcrawler-$version-standalone-win64.exe"
            }
        }
    }
}
